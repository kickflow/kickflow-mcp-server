{
  "name": "@kickflow/mcp-server",
  "version": "1.0.4",
  "description": "MCP Server for kickflow API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "@kickflow/mcp-server": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "url": "https://github.com/kickflow/kickflow-mcp-server"
  },
  "scripts": {
    "build": "tsc && chmod 755 dist/index.js",
    "dev": "node --loader ts-node/esm src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:inspector": "npx @modelcontextprotocol/inspector node dist/index.js",
    "typecheck": "tsc --noEmit",
    "update-schema": "curl -s https://developer.kickflow.com/rest/schema.yaml -o schema.yaml",
    "generate-api": "rm -rf ./src/kickflow-api/generated && orval --config orval.config.ts",
    "lint": "eslint . --ext .ts,.mts,.cts --fix",
    "format": "prettier --write \"**/*.{ts,mts,cts,js,mjs,cjs,json,md}\""
  },
  "keywords": [
    "kickflow",
    "api",
    "mcp",
    "server"
  ],
  "author": "kickflow, Inc.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "engines": {
    "node": ">=22.18.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.26.0",
    "axios": "1.13.5",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@types/node": "22.19.10",
    "@vitest/eslint-plugin": "1.6.7",
    "axios-mock-adapter": "2.1.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "globals": "17.3.0",
    "orval": "7.21.0",
    "prettier": "3.8.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.55.0",
    "vitest": "4.0.18",
    "yaml": "2.8.2"
  }
}
